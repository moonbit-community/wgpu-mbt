{
  "import": [],
  "native-stub": [
    "wgpu_stub.c"
  ],
  "targets": {
    "raw.mbt": [
      "native"
    ]
  },
  "pre-build": [
    {
      "input": [
        "../../vendor/wgpu-native/Cargo.toml"
